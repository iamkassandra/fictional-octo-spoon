# Function to calculate ASCII sum and reduction
def ascii_checksum(text):
    ascii_sum = sum(ord(char) for char in text)
    reduction = ascii_sum
    while reduction > 9 and reduction not in (11, 22, 33):
        reduction = sum(int(d) for d in str(reduction))
    return ascii_sum, reduction

