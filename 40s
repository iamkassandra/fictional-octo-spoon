def compute_time_essence(time_str, date_digits=[2,7,0,8,2,0,2,5]):
    time_digits = [int(d) for d in time_str]
    time_sum = sum(time_digits)
    time_reduction = time_sum
    while time_reduction > 9 and time_reduction not in (11, 22, 33):
        time_reduction = sum(int(d) for d in str(time_reduction))

    date_sum = sum(date_digits)
    date_reduction = date_sum
    while date_reduction > 9 and date_reduction not in (11, 22, 33):
        date_reduction = sum(int(d) for d in str(date_reduction))

    combined = time_reduction + date_reduction
    comb_red = combined
    while comb_red > 9 and comb_red not in (11, 22, 33):
        comb_red = sum(int(d) for d in str(comb_red))

    return (time_sum, time_reduction, date_sum, date_reduction, combined, comb_red)


