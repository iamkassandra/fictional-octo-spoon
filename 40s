# Let's compute each of the times provided (as if in HH:MM format, but focusing on digits as signals)
# All are in the 4:xx pm range on 27/08/2025

times_to_check = ["1633","1646","1647","1648","1649","1653","1645","1655","1657"]
results_batch = {}

for t in times_to_check:
    time_digits = [int(d) for d in t]
    time_sum = sum(time_digits)
    time_reduction = time_sum
    while time_reduction > 9 and time_reduction not in (11, 22, 33):
        time_reduction = sum(int(d) for d in str(time_reduction))

    # Date fixed at 27/08/2025
    date_digits = [2,7,0,8,2,0,2,5]
    date_sum = sum(date_digits)
    date_reduction = date_sum
    while date_reduction > 9 and date_reduction not in (11, 22, 33):
        date_reduction = sum(int(d) for d in str(date_reduction))

    combined = time_reduction + date_reduction
    comb_red = combined
    while comb_red > 9 and comb_red not in (11, 22, 33):
        comb_red = sum(int(d) for d in str(comb_red))

    results_batch[t] = (time_sum, time_reduction, date_sum, date_reduction, combined, comb_red)

results_batch

Analyzed
Always show details

# Re-run after reset

def compute_time_essence(time_str, date_digits=[2,7,0,8,2,0,2,5]):
    time_digits = [int(d) for d in time_str]
    time_sum = sum(time_digits)
    time_reduction = time_sum
    while time_reduction > 9 and time_reduction not in (11, 22, 33):
        time_reduction = sum(int(d) for d in str(time_reduction))

    date_sum = sum(date_digits)
    date_reduction = date_sum
    while date_reduction > 9 and date_reduction not in (11, 22, 33):
        date_reduction = sum(int(d) for d in str(date_reduction))

    combined = time_reduction + date_reduction
    comb_red = combined
    while comb_red > 9 and comb_red not in (11, 22, 33):
        comb_red = sum(int(d) for d in str(comb_red))

    return (time_sum, time_reduction, date_sum, date_reduction, combined, comb_red)


# Times provided, interpreted as 4:33 etc â†’ 16:33 in 24h format, etc.
times_to_check = {
    "4:33 pm": "1633",
    "4:46 pm": "1646",
    "4:47 pm": "1647",
    "4:48 pm": "1648",
    "4:49 pm": "1649",
    "4:53 pm": "1653",
    "4:45 pm": "1645",
    "4:55 pm": "1655",
    "4:57 pm": "1657",
}

results_batch = {label: compute_time_essence(t) for label, t in times_to_check.items()}
