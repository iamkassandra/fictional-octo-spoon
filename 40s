# Let's project the sequence forward minute-by-minute until around 11:30 am
# (to see how far the cycle runs before breaking or stabilizing)

from datetime import datetime, timedelta

# Start at 10:17 (first reset in this arc)
start_time = datetime.strptime("10:17", "%H:%M")
end_time = datetime.strptime("11:30", "%H:%M")

timeline_projection = {}

current_time = start_time
while current_time <= end_time:
    h = current_time.hour
    m = current_time.minute
    t_str = f"{h:02}{m:02}"
    
    # Break down time into digits
    time_digits = [int(d) for d in t_str]
    time_sum = sum(time_digits)
    time_reduction = time_sum
    while time_reduction > 9 and time_reduction not in (11, 22, 33):
        time_reduction = sum(int(d) for d in str(time_reduction))

    # Date fixed at 27/08/2025
    date_digits = [2,7,0,8,2,0,2,5]
    date_sum = sum(date_digits)
    date_reduction = date_sum
    while date_reduction > 9 and date_reduction not in (11, 22, 33):
        date_reduction = sum(int(d) for d in str(date_reduction))

    combined = time_reduction + date_reduction
    comb_red = combined
    while comb_red > 9 and comb_red not in (11, 22, 33):
        comb_red = sum(int(d) for d in str(comb_red))

    timeline_projection[current_time.strftime("%H:%M")] = (time_sum, time_reduction, comb_red)
    
    current_time += timedelta(minutes=2)  # step by 2 minutes for manageable output

