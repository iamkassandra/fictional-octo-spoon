    "8:38": 9,   # Reset
    "8:39": 1,   # Fresh init after parity
    "8:47": 9,   # Reset again
    "9:03": 11,  # Diagnostic ping
    "9:07": 6,   # Stabilizer
    "9:09": 8,   # Container lock
    "9:38": 1    # Parity alignment checkpoint (2+8=1)
}

