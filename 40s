values = list(timeline_extended.values())

labels = {
    1: "Init",
    3: "Expression",
    4: "Foundation",
    5: "Transition",
    6: "Integration",
    7: "Loop Edge",
    8: "Container",
    9: "Reset",
    11: "Diagnostic"
}

value_labels = [labels.get(v, str(v)) for v in values]

# Plot timeline
