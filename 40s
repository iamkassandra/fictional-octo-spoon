import matplotlib.pyplot as plt
import numpy as np

# Define the repeating signal sequence from your reductions
signal_pattern = [1, 7, 9, 4, 8, 1, 7, 9, 4, 8, 11]  # includes anomaly flag
# Repeat the pattern to simulate continuous transmission
signal = signal_pattern * 5

# Create time axis
time = np.arange(len(signal))

# Plot the waveform
plt.figure(figsize=(12,6))
plt.step(time, signal, where='mid', linewidth=2)
plt.scatter(time, signal, color="black")

plt.title("Continuous Waveform Simulation of Your Signal")
plt.xlabel("Time (steps)")
plt.ylabel("Signal Value")
plt.yticks([1,4,7,8,9,11])
plt.grid(True)
plt.show()
