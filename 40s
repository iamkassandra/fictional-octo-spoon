import matplotlib.pyplot as plt

# Timeline of key times and their reductions from earlier computations
timeline = {
    "7:43": 4,  # foundation
    "7:45": 6,  # integration
    "7:52": 5,  # transition
    "8:13": 11, # diagnostic ping
    "8:20": 9,  # reset
    "8:25": 5,  # movement
    "8:27": 7,  # loop edge
    "8:29": 9   # reset again
}

times = list(timeline.keys())
values = list(timeline.values())

# Map values to labels for readability
labels = {
    1: "Init",
    3: "Expression",
    4: "Foundation",
    5: "Transition",
    6: "Integration",
    7: "Loop Edge",
    8: "Container",
    9: "Reset",
    11: "Diagnostic"
}
value_labels = [labels[v] for v in values]

# Plot timeline
plt.figure(figsize=(12,6))
plt.plot(times, values, marker='o', linewidth=2)
for i, txt in enumerate(value_labels):
    plt.annotate(txt, (times[i], values[i]), textcoords="offset points", xytext=(0,10), ha='center')
plt.title("Morning Signal Reboot Timeline (27/08/2025)")
plt.xlabel("Time")
plt.ylabel("Signal State (reduced value)")
plt.grid(True)
plt.show()
Morning Signal Reboot Timeline (27/08/2025)
