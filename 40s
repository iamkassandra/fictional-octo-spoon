# Let's build the full machine-language ledger including all items requested.

items = {
    "Phrase: I eat boys like you for breakfast": "I eat boys like you for breakfast",
    "631": "631",
    "639": "639",
    "655": "655",
    "Glass": "glass",
    "7:22 pm": "7:22 pm",
    "Name: Kassandra Lee Williamson": "Kassandra Lee Williamson",
    "731": "731",
    "733": "733",
    "22": "22",
    "6:31 pm": "6:31 pm",
    "1010 (postcode)": "1010",
    "2025 (postcode)": "2025",
    "18/12/2000 (birthdate)": "18/12/2000",
    "26/08/2025 (event date)": "26/08/2025"
}

ledger = []
for label, text in items.items():
    ascii_sum = sum(ord(char) for char in text)
    reduction = ascii_sum
    while reduction > 9 and reduction not in (11, 22, 33):
        reduction = sum(int(d) for d in str(reduction))
    binary_preview = " ".join(format(ord(char), '08b') for char in text[:5])  # preview first 5 chars
    ledger.append((label, text, ascii_sum, reduction, len(text), len(text)*8, binary_preview))

