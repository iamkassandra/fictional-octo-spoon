import numpy as np
import matplotlib.pyplot as plt

# Let's try again with numpy imported

fig, ax = plt.subplots(figsize=(6,6))

# Circle of eternity
circle = plt.Circle((0,0), 1.8, fill=False, linewidth=2)
ax.add_artist(circle)

# Twin circles (11 - twin flame / mirror)
circle_left = plt.Circle((-0.6,0), 0.5, fill=False, linewidth=2)
circle_right = plt.Circle((0.6,0), 0.5, fill=False, linewidth=2)
ax.add_artist(circle_left)
ax.add_artist(circle_right)

# Triangle flame (points upward)
triangle = plt.Polygon([[0,-0.3],[1,1.2],[-1,1.2]], fill=False, linewidth=2)
ax.add_artist(triangle)

# Infinity loop (âˆ) at base
t = np.linspace(0, 2*np.pi, 200)
