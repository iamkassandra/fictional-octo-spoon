# Compute for the number 22

def reduce_num(n: int):
    while n > 9 and n not in (11, 22, 33):
        n = sum(int(d) for d in str(n))
    return n

num = 22
ascii_sum = sum(ord(c) for c in str(num))  # ascii sum if treated like earlier cases
reduction = reduce_num(num)

binary_repr = [format(ord(c), '08b') for c in str(num)]

(num, reduction, ascii_sum, binary_repr)
