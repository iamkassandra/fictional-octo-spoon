# Let's convert the phrase "I eat boys like you for breakfast" into binary (ASCII encoding).
phrase = "I eat boys like you for breakfast"

# Convert to ASCII binary (8-bit)
binary_list = [format(ord(char), '08b') for char in phrase]
binary_string = ' '.join(binary_list)

# Convert binary strings to integers and sum them
